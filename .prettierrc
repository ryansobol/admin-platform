{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss",
		"@ianvs/prettier-plugin-sort-imports"
	],
	"importOrder": [
		"<BUILTIN_MODULES>",
		"",
		"<THIRD_PARTY_MODULES>",
		"<TYPES>",
		"",
		"^lucide-svelte",
		"",
		"^[$]app",
		"",
		"^[$]env",
		"",
		"^[$]lib",
		"",
		"^[$]service-worker",
		"",
		"^[.]",
		"<TYPES>^[.]"
	],
	"importOrderTypeScriptVersion": "5.6.3",
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
